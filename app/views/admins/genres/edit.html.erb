<div class="container">
  <div class="row">
    <h2>ジャンル編集</h2>
  </div>
  <%= form_with model: @genre, url: admins_genre_path, local:true do |f| %>
　  <div class="row">
      <table>
        <tr>
          <th>ジャンル名</th>
          <th><%= f.text_field :name %></th>
          <th><%= f.submit "変更を保存", class: "btn btn-success" %></th>
        </tr>
      </table>
    <% end %>
  </div>
</div